{
  "buildCommand": "npm run build && npm run build:sitemap",
  "outputDirectory": "dist",
  "env": {
    "VITE_API_BASE_URL": "https://type-soul-codes-api.vercel.app",
    "VITE_SITE_NAME": "Type Soul Codes",
    "VITE_SITE_DESCRIPTION": "Type Soul",
    "VITE_LOCAL_API_URL": "https://type-soul-codes-api.vercel.app",
    "VITE_NODE_ENV": "production"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
